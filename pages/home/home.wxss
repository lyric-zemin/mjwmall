@import "/common/icon/home";

page {
  background-color: var(--color-background);
}

.head {
  background-color: var(--color-theme);
  padding: 16rpx 10rpx 0;
  box-sizing: border-box;
  height: 142rpx;
}

.head-wrap {
  display: flex;
  justify-content: space-between;
  align-items: center;
}

.head v-search {
  flex: 1;
}

.head .icon-xiazai46 {
  width: 54rpx;
  text-align: center;
  font-size: 54rpx;
  color: var(--color);
  margin-left: 18rpx;
}

.body {
  padding: 0 10rpx;
}
